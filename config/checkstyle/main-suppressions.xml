<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
    <suppress checks="Indentation" files="src/main/java/.*" />
    <suppress checks="FileTabCharacter" files="src/main/java/.*" />
    <suppress checks="WhitespaceAfter" files="src/main/java/.*" />
    <suppress checks="WhitespaceAround" files="src/main/java/.*" />
    <suppress checks="JavadocVariable" files="src/main/java/.*" message="Missing a Javadoc comment." />
</suppressions>
