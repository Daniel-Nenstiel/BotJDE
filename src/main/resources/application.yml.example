spring:
  profiles:
    active: default
  main:
    banner-mode: off
  application:
    name: BotJDE
  datasource:
    url: jdbc:postgresql://localhost:5432/botjde
    username: postgres
    password: postgres
  flyway:
    enabled: true
    locations: classpath:db/migration
logging:
  level:
    ROOT: WARN

token: "MyTokenHere"
app:
  config:
    source: yaml
  servers:
    - id: 123456789012345678
      name: serverName
      defaultChannelId: 123456789012345678
      puzzleChannelId: 123456789012345678
      birthdaysEnabled: false
      anniversariesEnabled: false
      puzzlesEnabled: false

    - id: 234567890123456789
      name: secondServerName
      defaultChannelId: 234567890123456789
      puzzleChannelId: 234567890123456789
      birthdaysEnabled: true
      anniversariesEnabled: true
      puzzlesEnabled: true

  pocketSheets:
    - serverId: 123456789012345678
      urls:
        - name: DJ
          url: https://docs.google.com/spreadsheets/d/asdfsadf/edit
        - name: JD
          url: https://docs.google.com/spreadsheets/d/lkjfdsalkjfdsa/edit


